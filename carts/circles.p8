pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
 d=0
 rad=1
 res=10
 offset=0
 siz=0.5
 t=0
end
function _update60()
rad*=1.1^1.5
siz*=1.08^1.5
offset+=0.018
t+=1/60
if rad>120 then
rad=1
offset+=0.1
siz=0.5
end
end
function _draw()
 cls()
 for i = 0,res do
  circfill(64+sin(i/res+offset)*rad,64+cos(i/res+offset)*rad,siz,i+5)
 end
 spr(1,60+sin(t)*16,60+cos(t*1.1)*16)
end
